	// =============================================
	// =============================================
	// 	Methods:

	"java_shortPublicVoidMethod" : {
		"prefix": "+()",
		"body": [
			"public ${1:void} ${2:methodName} (${3:params}) {",
			"    $4",
			"}$0"
		],
		"description": "Java: public void method"
	},

	"java_shortPrivateVoidMethod" : {
		"prefix": "-()",
		"body": [
			"private ${1:void} ${2:methodName} (${3:params}) {",
			"    $4",
			"}$0"
		],
